print("Menu -> Area of Square,Area of Rectangle,Area of circle")
ch=input()

if (ch=="Area of Square"):
  side=int(input("Enter the side for area:"))
  AOS=side*side
  print("Area of square is {}".format(AOS))
elif ch=="Area of Rectangle":
